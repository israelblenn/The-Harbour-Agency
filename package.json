{
  "name": "the-harbour-agency",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build && cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap && cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.39.1",
    "@payloadcms/email-resend": "3.39.1",
    "@payloadcms/next": "3.39.1",
    "@payloadcms/payload-cloud": "3.39.1",
    "@payloadcms/richtext-lexical": "3.39.1",
    "@payloadcms/storage-s3": "3.39.1",
    "@payloadcms/ui": "3.39.1",
    "@tanstack/react-virtual": "^3.13.9",
    "cross-env": "^7.0.3",
    "fast-fuzzy": "^1.12.0",
    "flubber": "^0.4.2",
    "framer-motion": "^12.17.0",
    "graphql": "^16.8.1",
    "next": "15.6.0-canary.58",
    "next-view-transitions": "^0.3.4",
    "payload": "3.39.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-grid-layout": "^1.5.1",
    "react-virtualized": "^9.22.6",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/flubber": "^0.4.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "@types/react-virtualized": "^9.22.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "prettier": "^3.4.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
